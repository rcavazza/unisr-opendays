<% let queryString = ''; if (typeof totem !== 'undefined' && totem !== null &&
totem !== '') { queryString = `?totem=${totem}`; } else if (typeof banner !==
'undefined' && banner !== null && banner !== '') { queryString =
`?banner=${banner}`; } %> <% if (typeof hide !== 'undefined' && hide !== null &&
hide !== '') { if(queryString === ''){ queryString = `?hide=${hide}`; } else {
queryString = `${queryString}&hide=${hide}`; } }if (typeof qr !== 'undefined' &&
qr !== null && qr !== '') { if(queryString === ''){ queryString = `?qr=${qr}`; }
else { queryString = `${queryString}&qr=${qr}`; } } %>
